            font-size: 4.2rem;
            margin-bottom: 0px;

        }

        .job {
            padding-top: 5px;
        }

        #p2 {
            width: 52%;
            text-align: center;
        }

        .card1 {
            display: flex;
            flex-direction: column;
            width: 22%;
            height: 200px;
            align-items: center;
            margin: 33px;
            transition-duration: 350ms;
            border: 1px solid rgb(236, 234, 234);
            border-radius: 11px;
            background-color: rgb(246, 244, 244);
        }

        .card1 img {
            height: 17%;
        }

        .card1 p {
            text-align: center;
            margin: 0px 8px;
        }

        .infocards {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
        }

        .card1:hover {
            background-color: whitesmoke;
            scale: 1.08;
            box-shadow: inset;
        }
    </style>
    <script src="https://kit.fontawesome.com/e60263fb6b.js" crossorigin="anonymous"></script>
