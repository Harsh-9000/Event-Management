    <style>
        .sec1 {
            width: 100%;
            height: 50vh;
            background-color: aliceblue;
            position: relative;
            text-align: center;
        }


        .sec2 {
            width: 100%;
            height: 70vh;
            background-image: url(/Images/map.png);
            position: relative;
            text-align: center;

        }

        .sec2-1,
        .sec2-2 {
            position: relative;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            color: rgb(26 26 131 / 85%);
        }

        .hedding {
            display: flex;
            flex-direction: column;
            align-items: center;
        }

        #p1 {
